pipeline{
    agent any
    stages{
        stage("docker build"){
            steps{
                script{
                    sh "docker build -t unwash/primero:latest"
                }
            }
        }
        stage("docker push"){
            steps{
                script{
                    sh "docker push unwash/primero:1.0.0.${BUILD_ID}"
                }
            }
        }
    }
}